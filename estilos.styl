@font-face
  font-family 'icomoon'
  src url('fuentes/icomoon.eot?rft380'),
      url('fuentes/icomoon.ttf'),
      url('fuentes/icomoon.svg'),
      url('fuentes/icomoon.woff')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon'
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

