body
	font-size 16px 
	background #DDE3E6
 

a
	text-decoration none
   

ul
  padding 0
  margin 0
  list-style none 


  
header
	max-width 1200px
	background #05558d
	padding .4em 0
	#nav1
		position fixed 
		bottom 0
		width 100%
		background white
     
		border-top 1px solid gray
		ul
			font-size 0
		.item-nav
			width 50px
			display inline-block
			font-size 1rem
			border-right 1px solid #C7D3D8 
			padding 0 .4em
			list-style none	
			&:first-child
				border-left 0px
			&:last-child
				border-right 0px 
			.font-img
				font-size 1.6em
				color #0769AD
				margin-top .2em 	
	#nav2
		position absolute
		top 0
		right 0
		margin 0.5em
		border-radius 50%
		
		a
			color white
			font-size 1.5em
	#vistas
   
   width 100px
   margin 0 auto
   font-size 0
   p
    display inline-block
    font-size 1rem
    height 30px
    line-height 30px
    border 1px solid white
    background gray
    width 31%
    text-align center
    margin 0 auto
    margin-right 0
      
	.buscar
		padding .3em
		font-size 1em
    font-family "Lucida Grande", Lucida, Verdana, sans-serif  
		width 225px
    text-shadow 0 -1px 0 #000  
		display block
		border-radius .4em
		border 0px
  
  
		

.canvas_menu
  background #fff
  width 17em
  position fixed
  top 0
  left -19em
  transition .4s
  box-shadow 1px 1px 20px #333
  border-radius 0 0 .5em 0

  h3
    width 14.6em
    color white

  .canvas-item
    background #eee
    border 1 solid #eee
    margin .4em
    border-radius .3em

    a
      padding .3em 1em
      display block
      font-size 1.1em
      color #0769AD
  #info-usuario
    width 100%
    margin 0
    padding .5em 0
    top 0
    background #2359A1

    position relative
    h3
      color white
      margin 0
      margin-left .3em
    #logo-usuario
      position absolute
      right 0
      top 0
      margin 0
      margin-right 1em
      
      img
        border 1px solid transparent
        border-radius 50%
.contenedor-item
  background #fff
  margin 20px auto
  border 1px solid red
   
  .item-posted
    border-bottom 1px solid #22bce0
    padding .3em .2em
    overflow hidden
    margin 20px auto
    
    
    .img-item
      width 70px
      margin 0 .7em 0 1em
      
      background white
      float left
      img
        border-radius .7em 
    .info-item
      margin-top .7em
      
      .name-item
        margin 0
        font-size .8em
      .categoria-item
        margin 0
        font-size .7em
      
      .precio
        margin 0
    
.v2
  display inline-block
  width 48%
  box-shadow 2px 2px 5px red

.menu-mostrado
  left 0em 
  
.is-active
  display block
  
.not-active
  display none

.centrar-texto
	text-align center
	
.centrar-caja
	margin 0 auto

.color-titulos
  color #D4191A
.color-enlace
  color #D4191A

.red 
  position relative
  right 250px
  transition .3s